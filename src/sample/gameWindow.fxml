<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="rootPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <Label fx:id="titleLabel" focusTraversable="false" layoutX="8.0" layoutY="14.0" prefHeight="53.0" prefWidth="158.0" text="ここタイトル" textAlignment="CENTER">
         <font>
            <Font size="36.0" />
         </font>
      </Label>
      <Label fx:id="timerLabel" focusTraversable="false" layoutX="14.0" layoutY="67.0" prefHeight="25.0" prefWidth="147.0" text="ここ時間" textAlignment="CENTER" wrapText="true" />
      <ListView fx:id="memberListView" disable="true" focusTraversable="false" layoutX="14.0" layoutY="106.0" onMouseClicked="#memberSelected" prefHeight="206.0" prefWidth="147.0" />
      <Label fx:id="selectMemberLabel" disable="true" focusTraversable="false" layoutX="14.0" layoutY="332.0" prefHeight="39.0" prefWidth="102.0" text="選んだ人出る" wrapText="true" />
      <ListView fx:id="messageListView" focusTraversable="false" layoutX="175.0" layoutY="14.0" prefHeight="302.0" prefWidth="411.0" />
           <Button fx:id="messageSendButton" disable="true" focusTraversable="false" layoutX="500.0" layoutY="344.0" onAction="#messageSendButtonClick" text="送信" />
      <TextArea fx:id="messageTextArea" focusTraversable="false" layoutX="175.0" layoutY="326.0" prefHeight="63.0" prefWidth="318.0" promptText="名前を入力してください" wrapText="true" />
      <Button fx:id="memberSendButton" disable="true" focusTraversable="false" layoutX="122.0" layoutY="344.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="40.0" text="決定" />
      <Button fx:id="attendLeaveButton" layoutX="546.0" layoutY="345.0" mnemonicParsing="false" onAction="#attendLeaveButtonClick" text="参加" />
   </children>
</AnchorPane>
